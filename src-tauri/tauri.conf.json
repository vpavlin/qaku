{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "qaku",
  "version": "0.1.0",
  "identifier": "app.qaku",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Qaku",
        "width": 1024,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../public/logo.png",
      "../public/logo192.png",
      "../public/logo512.png"
    ],
    "externalBin": [
      "binaries/codex"
    ]
  }
}
